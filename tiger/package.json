{
	"name": "@isp.nexus/tiger",
	"version": "1.0.0",
	"description": "Utilities for working with Census TIGER data",
	"keywords": [
		"typescript"
	],
	"license": "AGPL-3.0",
	"contributors": [
		{
			"name": "Teffen Ellis",
			"email": "teffen@sister.software"
		}
	],
	"type": "module",
	"exports": {
		"./package.json": "./package.json",
		"./schema/*.json": {
			"types": "./dist/schema/*.d.ts",
			"import": "./dist/schema/*.json"
		},
		"./scripts/*": {
			"types": "./scripts/*.ts",
			"import": "./out/scripts/*.js"
		},
		"./sdk": {
			"types": "./out/sdk/index.d.ts",
			"import": "./out/sdk/index.js"
		},
		".": {
			"types": "./out/index.d.ts",
			"import": "./out/index.js"
		}
	},
	"scripts": {
		"test": "node ./out/sdk/TIGERService.test.js"
	},
	"dependencies": {
		"@isp.nexus/core": "workspace:*",
		"@isp.nexus/sdk": "workspace:*",
		"@isp.nexus/spatial": "workspace:*",
		"@turf/boolean-contains": "^7.0.0",
		"shapefile-parser": "^1.0.3",
		"type-fest": "^4.26.1"
	},
	"engines": {
		"node": ">=22.5.1"
	},
	"publishConfig": {
		"access": "public"
	}
}
