{
	"name": "@isp.nexus/schema",
	"version": "1.0.0",
	"description": "Schema definitions for the ISP Nexus API",
	"keywords": [
		"typescript"
	],
	"license": "AGPL-3.0",
	"contributors": [
		{
			"name": "Teffen Ellis",
			"email": "teffen@sister.software"
		}
	],
	"type": "module",
	"exports": {
		"./package.json": "./package.json",
		"./*.json": {
			"import": "./generated/*.json",
			"types": "./generated/*.json.d.ts"
		},
		"./sdk": {
			"types": "./out/sdk/index.d.ts",
			"import": "./out/sdk/index.js"
		},
		".": {
			"types": "./out/index.d.ts",
			"import": "./out/index.js"
		}
	},
	"scripts": {
		"generate": "node ./out/scripts/generate.js"
	},
	"devDependencies": {
		"@types/json-schema": "^7.0.15",
		"json-schema": "^0.4.0"
	},
	"engines": {
		"node": ">=22.5.1"
	},
	"publishConfig": {
		"access": "public"
	}
}
